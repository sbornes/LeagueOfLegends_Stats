{
  "name": "Jquery",
  "version": "1.0.0",
  "description": "Jquery package.json",

  "standard": {
    "globals": [
      "$"
    ]
  },

  "rules": {
        "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
    },

    "varsIgnorePattern": "[iI]gnored"
}
